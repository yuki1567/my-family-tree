{
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".output/**",
        ".nuxt/**"
      ]
    },
    "type-check": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "check": {
      "outputs": []
    },
    "test:unit": {
      "outputs": [],
      "env": [
        "APP_ENV"
      ]
    },
    "test:integration": {
      "cache": false,
      "outputs": [],
      "env": [
        "APP_ENV",
        "DATABASE_URL",
        "DATABASE_ADMIN_URL"
      ]
    },
    "quality": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}